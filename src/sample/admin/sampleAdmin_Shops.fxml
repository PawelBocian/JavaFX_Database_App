<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" style="-fx-background-color: #555353;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AdminShopController">
   <children>
      <TableView fx:id="shopsTable" layoutX="8.0" layoutY="8.0" prefHeight="383.0" prefWidth="783.0">
         <columns>
            <TableColumn fx:id="colID" prefWidth="51.0" text="ID" />
            <TableColumn fx:id="colName" prefWidth="140.0" text="Name" />
            <TableColumn fx:id="colDate" prefWidth="147.0" text="Date Of Establishment" />
            <TableColumn fx:id="colSurface" minWidth="0.0" prefWidth="100.0" text="Surface" />
            <TableColumn fx:id="colNumberOfAgencies" prefWidth="145.0" text="Number of Agencies" />
         </columns>
      </TableView>
      <JFXButton fx:id="addShopButton" buttonType="RAISED" layoutX="457.0" layoutY="399.0" onAction="#addShopButtononClicked" prefHeight="33.0" prefWidth="155.0" ripplerFill="WHITE" style="-fx-background-color: #3d9fcc;" text="Add Shop" textFill="WHITE" />
      <JFXButton fx:id="refreshShopButton" buttonType="RAISED" layoutX="457.0" layoutY="442.0" onAction="#refreshShopButtononClicked" prefHeight="31.0" prefWidth="308.0" ripplerFill="WHITE" style="-fx-background-color: #3d9fcc;" text="Refresh" textFill="WHITE" />
