      <JFXButton fx:id="BackShopButton" buttonType="RAISED" layoutX="21.0" layoutY="405.0" onAction="#backShopButtononClicked" prefHeight="75.0" prefWidth="155.0" ripplerFill="WHITE" style="-fx-background-color: #3d9fcc;" text=" &lt;--- Back" textFill="WHITE" />
   </children>
</AnchorPane>
